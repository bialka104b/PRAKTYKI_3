{{!-- TO BEDZIE RENDEROWANE DO PODSTRONY /editUser --}}

<div class="nameColumnResult row">
    <h1>{{title}}</h1>
    <h4>Tutaj możesz edytować dane Kandydata o id={{id}} &#128515;</h4>
    <div class="col-12 wiersz1">
        <div class="row kolumna2" id="editUser">
            <div class="persons black col-12 col-md-12">
                <form action="/saveDatabase" method="get">
                    <h3>Wprowadź nowe dane aby zaktualizować kandydata:</h3>
                    <span>Domyślnie są wpisane dotychczasowe dane</span>
                    {{#each person}}
                    <div class="opakowanie">
                        <label for="imieSave">Wpisz imię:</label>
                        <input type="text" name="imieSave" id="imieSave" value="{{Imie}}">
                    </div>
                    <div class="opakowanie">
                        <label for="nazwiskoSave">Wpisz nazwisko:</label>
                        <input type="text" name="nazwiskoSave" id="nazwiskoSave" value="{{Nazwisko}}">
                    </div>
                    <div class="opakowanie">
                        <label for="emailSave">Wpisz email:</label>
                        <input type="text" name="emailSave" id="emailSave" value="{{Email}}">
                    </div>
                    <div class="opakowanie">
                        <label for="telefonSave">Wpisz telefon:</label>
                        <input type="text" name="telefonSave" id="telefonSave" value="{{Telefon}}">
                    </div>
                    <div class="opakowanie">
                        <label for="miejscowoscSave">Wpisz miejscowosc:</label>
                        <input type="text" name="miejscowoscSave" id="miejscowoscSave" value="{{Miejscowosc}}">
                    </div>
                    <div class="opakowanie">
                        <label for="angielskiSave">Wpisz angielski w formacie "Angielski B1":</label>
                        <input type="text" name="angielskiSave" id="angielskiSave" value="{{Angielski}}">
                    </div>
                    <div class="opakowanie">
                        <label for="niemieckiSave">Wpisz niemiecki w formacie "Niemiecki B1":</label>
                        <input type="text" name="niemieckiSave" id="niemieckiSave" value="{{Niemiecki}}">
                    </div>
                    <div class="opakowanie">
                        <label for="pozostalejezykiSave">Wpisz pozostałe języki w formacie "Hiszpański B1, Rosyjski B2":</label>
                        <input type="text" name="pozostalejezykiSave" id="pozostalejezykiSave" value="{{Pozostale_Jezyki}}">
                    </div>
                    <div class="opakowanie">
                        <label for="wiekSave">Wpisz wiek w postaci liczby całkowitej:</label>
                        <input type="number" name="wiekSave" id="wiekSave" value="{{Wiek}}">
                    </div>
                    <div class="opakowanie">
                        <label for="relokacjaSave">Wpisz zainteresowanie relokacją "tak"/"nie":</label>
                        <input type="text" name="relokacjaSave" id="relokacjaSave" value="{{Relokacja}}">
                    </div>
                    <div class="opakowanie">
                        <label for="firmyWspolpracaSave">Dopisz firmy z jakimi kandydat współpracował":</label>
                        <input type="text" name="firmyWspolpracaSave" id="firmyWspolpracaSave" value="{{Firmy_Wspolpraca}}">
                    </div>
                    <div class="opakowanie">
                        <label for="doswiadczenieSave">Doświadczenie kandydata:</label>
                        <input type="text" name="doswiadczenieSave" id="doswiadczenieSave" value="{{Doswiadczenie}}">
                    </div>
                    <div class="opakowanie">
                        <label for="widelkiSave">Widełki kandydata:</label>
                        <input type="text" name="widelkiSave" id="widelkiSave" value="{{Widelki}}">
                    </div>
                    <div class="opakowanie">
                        <label for="technologieSave">Dopisz technologie - nie kasuj tego co jest wpisane jeśli nie jest to
                            konieczne":</label>
                        <input type="text" name="technologieSave" id="technologieSave" value="{{Technologie}}">
                    </div>
                    <div class="opakowanie">
                        <label for="specjalnoscSave">Wpisz specjalność kandydata np "FulStack Developer"</label>
                        <input type="text" name="specjalnoscSave" id="specjalnoscSave" value="{{Specjalnosc}}">
                    </div>
                    <div class="opakowanie">
                        <label for="dataKontaktuSave">Wprowadź ostatnią date kontaktu:{{Data_kontaktu}}</label>
                        <input type="date" name="dataKontaktuSave" id="dataKontaktuSave">
                    </div>
                    <div class="opakowanie">
                        <label for="komentarzeSave">Dopisz komentarz:</label>
                        <input type="text" name="komentarzeSave" id="komentarzeSave" value="{{Komentarze}}">
                    </div>
                    <div class="opakowanie">
                        <label for="linkDoProfiluSave">Wprowadź link linkedin</label>
                        <input type="text" name="linkDoProfiluSave" id="linkDoProfiluSave" value="{{Link_Do_Profilu}}">
                    </div>
                    <div class="opakowanie">
                        <label for="rozmowaTechnicznaSave">Rozmowa techniczna - wynik</label>
                        <input type="text" name="rozmowaTechnicznaSave" id="rozmowaTechnicznaSave" value="{{Rozmowa_techniczna}}">
                    </div>
                    <div class="opakowanie">
                        <label for="rozmowaNietechnicznaSave">Rozmowa nietechiczna wynik</label>
                        <input type="text" name="rozmowaNietechnicznaSave" id="rozmowaNietechnicznaSave" value="{{Rozmowa_nietechniczna}}">
                    </div>
                    {{/each}}
                    <label for="id">ID KANDYDATA</label>
                    <input type="text" name="idk" id="idk" value="{{id}}">
                    <button type="submit">wyslij</button>
                </form>
            </div>
            <a class="back_home" href="../">&#9194; Powróć do strony głównej</a>
        </div>
    </div>
</div>

